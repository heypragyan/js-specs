{
  "name": "@vadelabs/specs",
  "version": "0.1.6",
  "description": "Idiomatic spec library for NodeJS",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vadelabs/vade-spec.git"
  },
  "scripts": {
    "build": "babel src/ -d dist/ --source-maps",
    "build:watch": "babel src/ -d dist/ --watch --source-maps",
    "dev": "babel-watch src/server.js",
    "debug": "ndb nodemon dist/server.js",
    "start": "pm2 start pm2.json",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint src/ --fix"
    }
  },
  "keywords": [
    "nodejs",
    "spec",
    "clojure spec",
    "generative-testing",
    "property-based-testing",
    "simulation-testing",
    "vadelabs"
  ],
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "testMatch": [
      "**/__tests__/**/*.js"
    ]
  },
  "author": "Pragyan Tripathi <pragyan@vadelabs.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vadelabs/vade-spec/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/vadelabs/vade-spec#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-async-to-generator": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "babel-watch": "^7.0.0",
    "debug": "^4.1.1",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lerna": "^3.13.0",
    "prettier-eslint": "^8.8.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "jsonpath": "^1.0.1",
    "lodash": "^4.17.14",
    "shortid": "^2.2.14",
    "transducers-js": "^0.4.174"
  }
}
